{
  "expo": {
    "name": "LaunqStack",
    "slug": "launqstack",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-wordmark.png",
    "splash": {
      "backgroundColor": "#F7F7F9",
      "resizeMode": "contain"
    },
    "scheme": "launqstack",
    "platforms": [
      "ios",
      "android"
    ],
    "userInterfaceStyle": "light",
    "androidStatusBar": {
      "backgroundColor": "#F7F7F9",
      "barStyle": "dark-content",
      "translucent": false
    },
    "backgroundColor": "#F7F7F9",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.turbo.example",
      "infoPlist": {
        "UIViewControllerBasedStatusBarAppearance": false,
        "UIStatusBarStyle": "UIStatusBarStyleDarkContent"
      },
      "splash": {
        "backgroundColor": "#F7F7F9",
        "resizeMode": "contain"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#F7F7F9"
      },
      "splash": {
        "backgroundColor": "#F7F7F9",
        "resizeMode": "contain"
      },
      "package": "com.brucewaynegroup.launqstack"
    },
    "extra": {
      "eas": {
        "projectId": "00000000-0000-0000-0000-000000000000"
      }
    },
    "plugins": [
      "expo-maps"
    ]
  }
}