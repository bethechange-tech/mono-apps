{
  "expo": {
    "name": "LaunqStack",
    "slug": "launqstack",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-wordmark.png",
    "splash": {
      "backgroundColor": "#0A0F1A",
      "image": "./assets/logo-wordmark.png",
      "resizeMode": "contain"
    },
    "scheme": "launqstack",
    "platforms": [
      "ios",
      "android"
    ],
    "userInterfaceStyle": "dark",
    "androidStatusBar": {
      "backgroundColor": "#0A0F1A",
      "barStyle": "light-content",
      "translucent": false
    },
    "backgroundColor": "#0A0F1A",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.turbo.example",
      "infoPlist": {
        "UIViewControllerBasedStatusBarAppearance": false,
        "UIStatusBarStyle": "UIStatusBarStyleLightContent"
      },
      "splash": {
        "backgroundColor": "#0A0F1A",
        "image": "./assets/logo-wordmark.png",
        "resizeMode": "contain"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-wordmark.png",
        "backgroundColor": "#0A0F1A"
      },
      "splash": {
        "backgroundColor": "#0A0F1A",
        "image": "./assets/logo-wordmark.png",
        "resizeMode": "contain"
      },
      "package": "com.brucewaynegroup.launqstack"
    },
    "extra": {
      "eas": {
        "projectId": "00000000-0000-0000-0000-000000000000"
      }
    },
    "plugins": [
      "expo-maps"
    ]
  }
}
